	        }else{
	        	document.getElementById("select_year" + gyouNum).disabled = true;
	        	document.getElementById("select_month" + gyouNum).disabled = true;
	        	document.getElementById("select_day" + gyouNum).disabled = true;
	        }
		}
		//削除ボタン押下
		function delete_button(){
			document.search.action = "/delete/contractDelCheck"
			document.search.submit();
		}
		
		//更新ボタン押下
