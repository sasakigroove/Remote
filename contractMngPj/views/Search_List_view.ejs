<!DOCTYPE html>

<head>
	<title>sample</title>
	<meta http-equiv="content-type" charset="Shift_jis">
</head>
<style>
	/* Excel取り込みと出力ボタン */
	nav.top {
		text-align: center;
	}

	nav ul {
		margin: 0;
		padding: 0;
	}

	nav.top li {
		list-style: none;
		display: inline-block;
		width: 15%;
		min-width: 90px;
	}

	nav.top li:not(:last-child) {
		border-right: 2px solid #ddd;
	}

	nav.top a.linkbutton {
		text-decoration: none;
		color: #333;
	}

	nav.top a.linkbutton:hover {
		color: #507fc7;
		border-bottom: 2px solid #3862a1;
	}

	/* キーワード検索欄 */
	.cp_iptxt {

		height: 3em;
		width: 250px;
		overflow: hidden;
	}

	.cp_iptxt input[type='text'] {
		font: 15px/24px sans-serif;
		box-sizing: border-box;
		width: 100%;
		padding: 0.3em;
		transition: 0.3s;
		letter-spacing: 1px;
		color: #aaaaaa;
		border: 3.5px solid#999;
		border-radius: 2px;
	}

	.ef input[type='text']:focus {
		border: 3.5px solid #3879D9;
		outline: none;
	}


	/* タイトル */
	h3 {
		padding: 0.25em 0.5em;
		/*上下 左右の余白*/
		color: #3862a1;
		/*文字色*/
		background: transparent;
		/*背景透明に*/
		border-left: solid 5px #3862a1;
		/*左線*/
	}

	h4 {
		padding-top: 10px;
		padding-bottom: 10px;
		margin: 0px;

	}

	/* 検索欄プルダウン */
	.cp_ipselect1 {
		overflow: hidden;
		width: 30%;

	}

	.cp_ipselect1 select {
		width: 100%;
		padding-right: 1em;
		cursor: pointer;
		text-indent: 0.01px;
		text-overflow: ellipsis;
		border: none;
		outline: none;
		background: transparent;
		background-image: none;
		box-shadow: none;
		-webkit-appearance: none;
		appearance: none;
	}

	.cp_ipselect1 select::-ms-expand {
		display: none;
	}

	.cp_ipselect1.cp_sl03 {
		position: relative;
		border-radius: 2px;
		border: 2px solid #3862a1;
		background: #ffffff;
	}

	.cp_ipselect1.cp_sl03::before {
		position: absolute;
		top: 0.8em;
		right: 0.8em;
		width: 0;
		height: 0;
		padding: 0;
		content: '';
		border-left: 6px solid transparent;
		border-right: 6px solid transparent;
		border-top: 6px solid #3862a1;
		pointer-events: none;
	}

	.cp_ipselect1.cp_sl03 select {
		padding: 8px 38px 8px 8px;
		color: #3862a1;
	}


	/* 検索メニューのdiv */
	.div_float:after {
		position: relative;
		width: 100%;
		content: '';
		display: table;
		clear: both;
		position: relative;
		overflow: hidden;


	}

	.div_float .cell {
		position: relative;
		width: 800px;
		left: 30%;
		right: 25%;
		float: left;
	}


	/* 検索結果テーブルのスクロールバー */
	.ber {
		margin-left: 100px;
		height: 400px;
		overflow: scroll;
		overflow-x: hidden;
		overflow-y: auto;

	}

   /* ラジオボタン*/
	.ECM_RadioInput {
		padding: 12px 8px;
		display: flex;
		align-items: center;
		cursor: pointer;
	}

	.ECM_RadioInput:hover {
		background: rgba(0, 0, 0, 0.05);
	}

	.ECM_RadioInput-Input {
		opacity: 0;
		width: 0;
		margin: 0;
	}

	.ECM_RadioInput-Input:focus+.ECM_RadioInput-DummyInput {
		border: solid 2px #3862a1;
		background: #3862a1;
	}

	.ECM_RadioInput-Input:checked+.ECM_RadioInput-DummyInput {
		border: solid 2px #3862a1;
	}

	.ECM_RadioInput-Input:checked+.ECM_RadioInput-DummyInput::before {
		content: "";
		display: block;
		position: absolute;
		top: 50%;
		left: 50%;
		transform: translate(-50%, -50%);
		width: 7px;
		height: 7px;
		border-radius: 50%;
		background: #3862a1;
	}

	.ECM_RadioInput-DummyInput {
		position: relative;
		top: 0;
		left: 0;
		display: block;
		width: 10px;
		height: 10px;
		border-radius: 50%;
		background: #fff;
		border: solid 2px #3862a1;
	}

	.ECM_RadioInput-LabelText {
		margin-left: 12px;
		display: block;
		font-size: 18px;
		font-weight: bold;
	}


	/* 年月のプルダウン */
	.cp_ipselect {
		position: relative;
		width: 15%;
		margin: 2em auto;
		text-align: left;
	}

	.cp_sl06 {
		position: relative;
		font-family: inherit;
		background-color: transparent;
		width: 100%;
		padding: 10px 10px 10px 0;
		font-size: 18px;
		border-radius: 0;
		border: none;
		border-bottom: 1px solid #3862a1;
	}

	.cp_sl06:focus {
		outline: none;
		border-bottom: 1px solid #3862a1;
	}

	.cp_ipselect .cp_sl06 {
		appearance: none;
		-webkit-appearance: none
	}

	.cp_ipselect select::-ms-expand {
		display: none;
	}

	.cp_ipselect:after {
		position: absolute;
		top: 18px;
		right: 10px;
		width: 0;
		height: 0;
		padding: 0;
		content: '';
		border-left: 6px solid transparent;
		border-right: 6px solid transparent;
		border-top: 6px solid #3862a1;
		pointer-events: none;
	}

	.cp_sl06_selectlabel {
		color: #3862a1;
		font-size: 18px;
		font-weight: normal;
		position: absolute;
		pointer-events: none;
		left: 0;
		top: 10px;
		transition: 0.2s ease all;
	}

	.cp_sl06:focus~.cp_sl06_selectlabel,
	.cp_sl06:valid~.cp_sl06_selectlabel {
		color: #da3c41;
		top: -20px;
		transition: 0.2s ease all;
		font-size: 14px;
	}

	.cp_sl06_selectbar {
		position: relative;
		display: block;
		width: 100%;
	}

	.cp_sl06_selectbar:before,
	.cp_sl06_selectbar:after {
		content: '';
		height: 2px;
		width: 0;
		bottom: 1px;
		position: absolute;
		background: #3862a1;
		transition: 0.2s ease all;
	}

	.cp_sl06_selectbar:before {
		left: 50%;
	}

	.cp_sl06_selectbar:after {
		right: 50%;
	}

	.cp_sl06:focus~.cp_sl06_selectbar:before,
	.cp_sl06:focus~.cp_sl06_selectbar:after {
		width: 50%;
	}

	.cp_sl06_highlight {
		position: absolute;
		top: 25%;
		left: 0;
		pointer-events: none;
		opacity: 0.5;
	}


	/* 絞り込み検索実行ボタン */
	.btn,
	input.btn,
	button.btn {
		font-size: 1.0rem;
		font-weight: 600;
		line-height: 1.5;
		position: relative;
		display: inline-block;
		padding: 1rem 4rem;
		cursor: pointer;
		-webkit-user-select: none;
		-moz-user-select: none;
		-ms-user-select: none;
		user-select: none;
		-webkit-transition: all 0.3s;
		transition: all 0.3s;
		text-align: center;
		vertical-align: middle;
		text-decoration: none;
		letter-spacing: 0.1em;
		color: #3862a1;
		border-radius: 0.5rem;
	}

	input.btn-border {
		border: 2px solid #3862a1;
		border-radius: 0;
		background: #fff;
	}

	input.btn-border:hover {
		color: #fff;
		background: #3862a1;
	}



	/* 合計のテーブル */
	table.listDisp {
		width: 65%;
		border-spacing: 0;
	}

	table.listDisp th:first-child {
		top: 0;
		white-space: nowrap;
		border-radius: 5px 0 0 0;
		position: sticky;
	}

	table.listDisp th:last-child {
		top: 0;
		white-space: nowrap;
		border-radius: 0 5px 0 0;
		border-right: 1px solid #3c6690;
		position: sticky;
	}

	table.listDisp th {
		top: 0;
		white-space: nowrap;
		text-align: center;
		color: white;
		background: linear-gradient(#829ebc, #225588);
		border-left: 1px solid #3c6690;
		border-top: 1px solid #3c6690;
		border-bottom: 1px solid #3c6690;
		box-shadow: 0px 1px 1px rgba(255, 255, 255, 0.3) inset;
	
	}


	table.listDisp td {
		white-space: nowrap;
		text-align: center;
		border-left: 1px solid #a8b7c5;
		border-bottom: 1px solid #a8b7c5;
		border-top: none;
		padding: 8px 0;
	}

	table.listDisp td:last-child {
		border-right: 1px solid #a8b7c5;
	}

	/* テーブルのセル全体をクリック可能にする */
	a.widelink {
		text-decoration: none;
		color: #fff;
		display: block;
		/* リンクをブロックレベル化して表示 */
		padding: 0.6em 1em;
		/* 内側の余白量を上下に0.6文字分＆左右に1文字分にする */
	}

	a.widelink:hover {
		background-color: #5581ad;
	}


	/* 合計のテーブルタグ */
	table.result {
		width: 20%;
		border-collapse: separate;
		border-spacing: 0;
		margin-right:300px ;
	
	}

	table.result th {
		top: 0;
		white-space: nowrap;
		text-align: left;
		color: white;
		background: linear-gradient(#829ebc, #225588);
		border-left: 1px solid #3c6690;
		border-top: 1px solid #3c6690;
		border-bottom: 1px solid #3c6690;
		box-shadow: 0px 1px 1px rgba(255, 255, 255, 0.3) inset;
		position: relative;

	}

	table.result td {
		white-space: nowrap;
		text-align: center;
		border-left: 1px solid #a8b7c5;
		border-bottom: 1px solid #a8b7c5;
		border-top: 1px solid #a8b7c5;
		padding: 1px 0;
	}


	/* 更新と削除のボタン */
	nav.under {
		text-align: center;
		width: 100%;
		position: relative;
		/*←絶対位置*/
		bottom: 10px;
		/*下に固定*/
	}

	nav.under ul {
		margin: 0;
		padding: 0;
	}

	nav.under li {
		list-style: none;
		display: inline-block;
		width: 15%;
		min-width: 90px;
	}

	nav li:not(:last-child) {
		border-right: 2px solid #ddd;
	}

	nav a.linkbutton2 {
		text-decoration: none;
		color: #333;
	}

	nav a.linkbutton2:hover {
		color: #507fc7;
		border-bottom: 2px solid #507fc7;
	}
</style>
<script type="text/javascript">

	function onload_submit(){
		if (document.getElementById("errorcount").value == 0) {
			 if(document.getElementById("pushButton").value == 1){

			 }else if(document.getElementById("pushButton").value == 2){
				if(document.getElementById("warningcount").value == 0){
					document.search.action = "/delete/contractDel";
					document.search.submit();
				}else{
					var warningMsg = "";
					for(var i=0; i < document.getElementById("warningcount").value;i++){
						warningMsg += document.getElementById("warningmsg"+i).value+"\n";
					}
					if (window.confirm(warningMsg) ){
						document.getElementById("warningcount").value = 0;
						document.search.action = "/delete/contractDel";
						document.search.submit();
					}
				}
			 }else{
			 }
		}
	}







		//保刈追加2020/12/08end
		
	function SortAction(number) {


