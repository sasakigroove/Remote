		document.search.submit();
	}

        
        function inputControll(gyouNum){
        	if(document.getElementById("select_year" + gyouNum).disabled == true){
	        	document.getElementById("select_year" + gyouNum).disabled = false;
	        	document.getElementById("select_month" + gyouNum).disabled = false;
